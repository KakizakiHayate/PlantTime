<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.ac.jec.cm0110.plantime">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlanTime">
        <activity
            android:name=".SunActivity"
            android:exported="false" />
        <activity
            android:name=".SatActivity"
            android:exported="false" />
        <activity
            android:name=".FriActivity"
            android:exported="false" />
        <activity
            android:name=".ThuActivity"
            android:exported="false" />
        <activity
            android:name=".WedActivity"
            android:exported="false" />
        <activity
            android:name=".TueActivity"
            android:exported="false" />
        <activity
            android:name=".MonActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>